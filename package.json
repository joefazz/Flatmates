{
    "name": "Flatmates",
    "version": "0.1.0",
    "private": true,
    "devDependencies": {
        "apollo-client-preset": "^1.0.3",
        "detox": "^7.0.0-alpha.0",
        "enzyme": "^3.2.0",
        "enzyme-to-json": "^3.3.0",
        "eslint-plugin-react": "^7.4.0",
        "graphql": "^0.11.7",
        "graphql-tag": "^2.5.0",
        "jest": "^22.0.3",
        "react-addons-test-utils": "^15.6.2",
        "react-dom": "^16.2.0",
        "react-test-renderer": "16.0.0-beta.5",
        "subscriptions-transport-ws": "^0.9.1"
    },
    "scripts": {
        "ios": "react-native run-ios --simulator=\"iPhone X\"",
        "android": "react-native run-android",
        "emulator": "cd ~ && ./RunAndEmulate",
        "packager": "react-native start",
        "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf $TMPDIR/haste-map-react-native-packager-* && rm -rf node_modules/ && yarn cache clean && yarn",
        "shake": "adb shell input keyevent 82",
        "test": "jest",
        "postinstall": "rm ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json",
        "update-tests": "jest -u"
    },
    "jest": {
        "preset": "react-native",
        "setupFiles": [
            "./jest/setup.js"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!(react-native|Flatmates|@mapbox/react-native-mapbox-gl|react-native-image-crop-picker))"
        ],
        "testPathIgnorePatterns": ["__E2E__", "node_modules"]
    },
    "detox": {
        "test-runner": "jest",
        "runner-config": "Client/__E2E__/config.json",
        "configurations": {
            "ios.sim.debug": {
                "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Flatmates.app",
                "build": "xcodebuild -workspace ios/Flatmates.xcworkspace -scheme Flatmates -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
                "type": "ios.simulator",
                "name": "iPhone X"
            }
        }
    },
    "rnpm": {
        "assets": [
            "./Assets/Fonts"
        ]
    },
    "dependencies": {
        "@mapbox/react-native-mapbox-gl": "^6.0.2",
        "apollo-link-ws": "^1.0.2",
        "eslint-plugin-react-native": "^3.1.0",
        "immutable": "^3.8.2",
        "lodash": "^4.17.4",
        "moment": "^2.19.2",
        "randomcolor": "^0.5.3",
        "react": "16.2.0",
        "react-apollo": "^2.0.1",
        "react-native": "0.52.0",
        "react-native-animatable": "^1.2.4",
        "react-native-elements": "^0.18.2",
        "react-native-fast-image": "^2.0.1",
        "react-native-fbsdk": "^0.6.3",
        "react-native-image-crop-picker": "^0.19.1",
        "react-native-interactable": "^0.1.7",
        "react-native-swiper": "^1.5.13",
        "react-native-vector-icons": "^4.5.0",
        "react-navigation": "^1.0.0-beta.19",
        "react-redux": "^5.0.6",
        "redux": "^3.7.2",
        "redux-devtools-extension": "^2.13.2",
        "redux-immutablejs": "^0.0.8",
        "redux-logger": "^3.0.6",
        "redux-persist": "4.10.2",
        "redux-persist-immutable": "^4.3.1",
        "redux-saga": "^0.16.0",
        "redux-saga-routines": "^2.2.2"
    }
}
